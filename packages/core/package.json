{
  "name": "@gestaltjs/core",
  "version": "0.7.10",
  "private": false,
  "description": "Set of Getalt tools and models that are shared across all the features",
  "keywords": [
    "gestalt",
    "gestaltjs",
    "web"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    "./common/*": {
      "node": "./dist/common/*.js",
      "types": "./dist/common/*.d.ts"
    },
    "./node/*": {
      "node": "./dist/node/*.js",
      "types": "./dist/node/*.d.ts"
    },
    "./testing/*": {
      "node": "./dist/testing/*.js",
      "types": "./dist/testing/*.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prepack": "cross-env NODE_ENV=production pnpm run build",
    "build": "pnpm clean && tsc -b tsconfig.build.json",
    "lint": "prettier -c src/** && eslint './**/*.{js,ts}'",
    "lint:fix": "prettier -w src/** && eslint './**/*.{js,ts}' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "tsc": "tsc && echo",
    "postpublish": "pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ],
    "rules": {
      "import/no-nodejs-modules": [
        "off"
      ]
    }
  },
  "peerDependencies": {
    "electron": "^18.2.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "electron": {
      "optional": true
    }
  },
  "dependencies": {
    "@oclif/core": "1.9.5",
    "acorn": "8.8.0",
    "case-anything": "2.1.10",
    "commondir": "1.0.1",
    "electron": "19.0.10",
    "fast-glob": "3.2.11",
    "fast-json-stringify": "5.0.1",
    "fastify": "4.1.0",
    "find-up": "6.3.0",
    "fs-extra": "10.1.0",
    "handlebars": "4.7.7",
    "inquirer": "9.0.2",
    "listr2": "5.0.1",
    "magic-string": "0.26.2",
    "noop-stream": "1.0.0",
    "path-equal": "1.2.2",
    "pathe": "0.3.2",
    "picocolors": "1.0.0",
    "pino": "8.1.0",
    "pino-abstract-transport": "1.0.0",
    "pino-pretty": "8.1.0",
    "plur": "5.1.0",
    "radix3": "0.1.2",
    "rehype-parse": "8.0.4",
    "rehype-stringify": "9.0.3",
    "source-map-support": "0.5.21",
    "stacktracey": "2.1.8",
    "tempy": "2.0.0",
    "terminal-link": "3.0.0",
    "ts-error": "1.0.6",
    "unified": "10.1.2",
    "vite": "3.0.0"
  },
  "devDependencies": {
    "@types/estree": "1.0.0",
    "@types/inquirer": "8.2.1"
  },
  "engine-strict": true,
  "engines": {
    "node": "^14.13.1 || >=16.0.0 || >=17.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ]
}
